                    @Html.DisplayFor(modelItem => nv.luongnv)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => nv.matkhau)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = nv.manv }) |
